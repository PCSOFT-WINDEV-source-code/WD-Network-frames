   -
     name : CheckPorts
     procedure_id : 1227213104552980244
     type_code : 14
     code : |1-
      // Summary: <specify the action of the procedure>
      // Syntax:
      //[ <Result> = ] CheckPorts (<sPortList> is string)
      //
      // Parameters:
      //	sPortList (string): <specify the role of sPortList>
      // Return value:
      // 	boolean: // 	None
      //
      // Example:
      // Indicate an example.
      //
      Procedure CheckPorts(sPortList is string)
      
      // Variable
      sPort is string
      
      // Empty string, no port is filtered
      IF sPortList = “” RESULT True
      
      FOR EACH string sPort of sPortList SEPAREE by “;”
      	// If Val = 0, it means that it is not a number that was inserted
      	IF Val(sPort) = 0 THEN
      		RESULT False
      	END
      END
      
      RESULT True
     type : 458752
