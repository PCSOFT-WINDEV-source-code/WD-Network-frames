        internal_properties : CAAAAAgAAABv5oWhPOvSZOcyXq6VseLWM/gxl0Pc3gAHi3OJWspsXuTrgEZPwC/zioFYJFG2mK1gmRgz9bdRLWt2oTSimxmOb55nl8z88JzZS1b+h7dFALUrJVOX1vRdY/9r8AVnwR+PDIbgxAhY6fZ9+deDpPmV93GeRtpoSXsht76D9KLY
        code : |1+
         
         // IGMP filtered or not?
         IF CBOX_FilteringOptions[1]..Value = True THEN
         	clMainCapture:CaptureIGMP(True)
         ELSE
         	clMainCapture:CaptureIGMP(False)
         END
         
         // ICMP filtered or not?
         IF CBOX_FilteringOptions[2]..Value = True THEN
         	clMainCapture:CaptureICMP(True)
         ELSE
         	clMainCapture:CaptureICMP(False)
         END
         
         // UDP and TCP filtered or not? If they are filtered, pass the requested ports in parameter for the capture
         // They must be separated by a;
         IF CBOX_FilteringOptions[3]..Value = True THEN
         	IF CheckPorts(EDT_PortUDP) THEN
         		clMainCapture:CaptureUDP(True,EDT_PortUDP)
         	ELSE
         		Error(<§$0001§>+CR+CR+<§$0003§>+CR+CR+<§$0006§>)
         		RETURN
         	END
         ELSE
         	clMainCapture:CaptureUDP(False)
         END
         
         IF CBOX_FilteringOptions[4]..Value = True THEN
         	IF CheckPorts(EDT_PortTCP) THEN
         		clMainCapture:CaptureTCP(True,EDT_PortTCP)
         	ELSE
         		Error(<§$0002§>+CR+CR+<§$0004§>+CR+CR+<§$0005§>)
         		RETURN
         	END
         ELSE
         	clMainCapture:CaptureTCP(False)
         END
         
         // Save the parameters
         SaveParameter(“IGMP”, CBOX_FilteringOptions[1]..Value)
         SaveParameter(“ICMP”, CBOX_FilteringOptions[2]..Value)
         SaveParameter(“UDP”, CBOX_FilteringOptions[3]..Value)
         SaveParameter(“TCP”, CBOX_FilteringOptions[4]..Value)
         SaveParameter(“PortUDP”, EDT_PortUDP)
         SaveParameter(“PortTCP”, EDT_PortTCP)
         
         // Close the window
         Close(WIN_Filtering)
